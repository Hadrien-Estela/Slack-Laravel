{
    "name": "virtuose-reality/slack-laravel",
    "description": "Slack API integration for Laravel framework.",
    "type": "package",
    "keywords": [
        "laravel",
        "slack"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hadrien ESTELA",
            "email": "hadrien@virtuosereality.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3.0",
        "symfony/console": "^5.0",
        "illuminate/console": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/notifications": "^8.0",
        "illuminate/http": "^8.0",
        "nesbot/carbon": "^2.41",
        "ulrichsg/getopt-php": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Slack\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slack\\Providers\\SlackServiceProvider"
            ],
            "aliases": {
                "Slack": "Slack\\Facades\\Slack"
            }
        }
    }
}
